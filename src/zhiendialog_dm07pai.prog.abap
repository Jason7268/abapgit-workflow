MODULE exit_command_0100 INPUT.
  CASE scr_okcode. "SY-ucomm. "
    WHEN   'BACK' OR 'CANC'.
      LEAVE TO SCREEN 0.
    WHEN 'EXIT'.
      LEAVE PROGRAM.

    WHEN OTHERS.
  ENDCASE.
ENDMODULE.
